<h1>Norse Gods</h1>

{{#each gods_for_index_view as |god|}}
  <div class="god">
    <div class="god-name">
      <a href="/gods_namespace/{{god.name}}">{{god.name}}</a>
    </div>

    <div class="god-domains">
      {{#each god.domains as |domain|}}
        <div class="god-domain">
          {{domain}}
        </div>
      {{/each}}<!-- domains -->
    </div>
  </div>
{{/each}}<!-- gods -->